<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <JFXTextField fx:id="nameTextField" focusColor="#276899" labelFloat="true" promptText="Name">
            <VBox.margin>
                <Insets bottom="10.0" left="30.0" right="30.0" top="30.0" />
            </VBox.margin>
        </JFXTextField>

        <JFXTextField fx:id="apartmentNumberTextField" focusColor="#276899" labelFloat="true" promptText="Apartment Number">
            <VBox.margin>
                <Insets bottom="10.0" left="30.0" right="30.0" top="15.0" />
            </VBox.margin>
        </JFXTextField>


        <JFXTextField fx:id="phoneTextField" focusColor="#276899" labelFloat="true" promptText="Phone">
            <VBox.margin>
                <Insets bottom="10.0" left="30.0" right="30.0" top="15.0" />
            </VBox.margin>
        </JFXTextField>
        <JFXTextField fx:id="emailTextField" focusColor="#276899" labelFloat="true" promptText="Email">
            <VBox.margin>
                <Insets bottom="20.0" left="30.0" right="30.0" top="15.0" />
            </VBox.margin>
        </JFXTextField>
        <JFXButton fx:id="saveChangesButton" style="-fx-background-color: #276899;" text="Save" textFill="WHITE">
            <VBox.margin>
                <Insets bottom="30.0" top="20.0" />
            </VBox.margin>
        </JFXButton>
    </children>
</VBox>
